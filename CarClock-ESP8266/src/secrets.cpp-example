//
//  Local include files :
//
#include "config.h"


//-----------------------------------------------------------------------------
//
//	secretsSetup() -- Routine to initialize secret stuff.
//
//-----------------------------------------------------------------------------

void secretsSetup ( void )
{
    MyPrintf ( "[secretsSetup]  Called.\n" );

    //
    //  First WiFi network we will attempt to connect to :
    //
    snprintf ( config.wifiSlots[0].ssid, sizeof(config.wifiSlots[0].ssid), "My_Home_WiFi" );
    snprintf ( config.wifiSlots[0].pass, sizeof(config.wifiSlots[0].pass), "somePassword" );

    //
    //  Second WiFi network we will attempt to connect to :
    //
    snprintf ( config.wifiSlots[1].ssid, sizeof(config.wifiSlots[0].ssid), "My_Phone_WiFi" );
    snprintf ( config.wifiSlots[1].pass, sizeof(config.wifiSlots[0].pass), "someOtherPassword" );
}


